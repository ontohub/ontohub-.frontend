{
    "editor.tabSize": 2,
    "files.insertFinalNewline": true,
    "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/.DS_Store": true,
      "**/node_modules": true,
      "**/.chrome": true
    },
    "eslint.enable": true,
    "prettier.singleQuote": true,
    "prettier.tabWidth": 2,
    "prettier.semi": false
  }
