// Place your settings in this file to overwrite default and user settings.
// Recommended Plugins:
// - ESLint
// - Prettier
// - Debugger for Chrome
// - Auto Close Tag
{
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "files.insertFinalNewline": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/.chrome": true,
    "**/coverage": true
  },
  "eslint.enable": true,
  "prettier.tabWidth": 2
}
