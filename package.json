    "backendVersion": "> 0.0.0-90"
  },
  "private": true,
  "license": "AGPL-3.0",
  "dependencies": {
    "lodash": "^4.17.4",
    "react": "^16.0.0",
    "react-apollo": "^1.4.15",
    "react-async-script": "^0.9.1",
    "react-css-transition-replace": "^3.0.0",
    "react-dom": "^15.6.0",
    "react-google-recaptcha": "^0.9.7",
    "react-gravatar": "^2.6.3",
    "react-router-dom": "^4.2.2",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.74.2",
    "semver": "^5.4.1",
    "styled-components": "^2.1.2",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@storybook/addon-info": "^3.2.11",
    "@storybook/react": "^3.2.10",
    "codecov": "^2.2.0",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^2.0.1",
    "eslint": "^4.7.2",
    "flow-bin": "^0.49.1",
    "flow-typed": "^2.1.2",
    "prettier-eslint-cli": "^4.0.3",
    "react-scripts": "1.0.13",
    "react-test-renderer": "^16.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "flow": "flow",
    "lint": "eslint .",
    "format": "prettier-eslint --write \"src/**/*.js\"",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/*.js",
      "!src/**/__stories__/*.js",
      "!src/**/components/index.js",
      "!src/**/scenes/index.js",
      "!src/**/containers/*.js",
      "!src/apollo/**/*.js",
      "!src/helpers/index.js"
    ]
  }
}
